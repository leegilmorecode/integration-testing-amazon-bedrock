{
  "name": "customer-api",
  "version": "0.1.0",
  "bin": {
    "customer-api": "bin/customer-api.js"
  },
  "scripts": {
    "deploy:develop": "STAGE=develop cdk deploy --all",
    "synth:develop": "STAGE=develop cdk synth --all",
    "remove:develop": "STAGE=develop cdk destroy --all",
    "deploy:test": "STAGE=test cdk deploy --all",
    "synth:test": "STAGE=test cdk synth --all",
    "remove:test": "STAGE=test cdk destroy --all",
    "test:e2e": "STAGE=test AWS_REGION=eu-west-2 jest --config ./jest.config.e2e.js --runInBand",
    "test:e2e:watch": "STAGE=test AWS_REGION=eu-west-2 jest --config ./jest.config.e2e.js --watch --runInBand"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "22.5.4",
    "@types/uuid": "^10.0.0",
    "aws-cdk": "2.162.1",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.10.0",
    "@aws-lambda-powertools/metrics": "^2.10.0",
    "@aws-lambda-powertools/parameters": "^2.10.0",
    "@aws-lambda-powertools/tracer": "^2.10.0",
    "@aws-sdk/client-bedrock-runtime": "^3.682.0",
    "@aws-sdk/client-dynamodb": "^3.682.0",
    "@aws-sdk/client-eventbridge": "^3.682.0",
    "@aws-sdk/client-scheduler": "^3.682.0",
    "@aws-sdk/client-ses": "^3.682.0",
    "@aws-sdk/client-sns": "^3.682.0",
    "@aws-sdk/client-ssm": "^3.682.0",
    "@aws-sdk/util-dynamodb": "^3.682.0",
    "@fastify/aws-lambda": "^5.0.0",
    "@middy/core": "^5.5.1",
    "@middy/http-error-handler": "^5.5.1",
    "@middy/sqs-partial-batch-failure": "^5.5.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "aws-cdk-lib": "2.162.1",
    "aws-jwt-verify": "^4.0.1",
    "axios": "^1.7.7",
